{
  "version": 2,
  "functions": {
    "server/index.ts": {
      "maxDuration": 10,
      "includeFiles": "dist/public/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/server/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1"
  }
}